# download via curl
curl https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js -o ./jquery.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/uikit.min.js -o ./uikit.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/components/sticky.min.js -o ./sticky.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/core/offcanvas.min.js -o ./offcanvas.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/components/slideshow.min.js -o ./slideshow.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/core/smooth-scroll.min.js -o ./smooth-scroll.min.js

curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/uikit.min.css -o ./uikit.min.css;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/components/sticky.min.css -o ./sticky.min.css;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/uikit.gradient.min.css -o ./uikit.gradient.min.css;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/components/slideshow.gradient.min.css -o ./slideshow.gradient.min.css;
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/components/dotnav.gradient.min.css -o ./dotnav.gradient.min.css
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/css/components/slidenav.gradient.min.css -o ./slidenav.gradient.min.css

curl https://cdnjs.cloudflare.com/ajax/libs/snap.svg/0.4.1/snap.svg-min.js -o ./snap.svg-min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js -o ./jquery-ui.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css -o ./jquery-ui.min.css;
curl https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js -o ./modernizr.min.js;
curl https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css -o ./normalize.min.css;
curl https://www.google-analytics.com/analytics.js -o ./analytics.js
<!-- Google Analytics: change UA-XXXXX-Y to be your site's ID. -->
<script>
window.ga=function(){ga.q.push(arguments)};ga.q=[];ga.l=+new Date;
ga('create','UA-XXXXX-Y','auto');ga('send','pageview')
</script>

curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/components/grid.min.js -o ./grid.min.js
curl https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/core/grid.min.js -o ./grid.min.js

curl https://opensource.keycdn.com/fontawesome/4.7.0/font-awesome.min.css -o ./font-awesome.min.css
curl https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/fonts/fontawesome-webfont.woff2 -o ./fontawesome-webfont.woff2
curl https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/fonts/fontawesome-webfont.woff -o ./fontawesome-webfont.woff
curl https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/fonts/fontawesome-webfont.ttf -o ./fontawesome-webfont.ttf
curl https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/fonts/fontawesome-webfont.ttf -o ./fontawesome-webfont.ttf

curl https://js.stripe.com/v2/ -o ./stripe.min.js

